							"// Verifica o JSON da resposta\r",
							"console.log(\"Resposta JSON:\", pm.response.json());\r",
							"\r",
							"// Armazena o token Bearer na variável de ambiente \"bearer_token\"\r",
							"var jsonData = pm.response.json();\r",
							"if (jsonData.access_token) {\r",
							"    //pm.environment.set(\"bearer_token\", jsonData.access_token);\r",
							"        pm.collectionVariables.set(\"bearer_token\", jsonData.access_token)\r",
							"\r",
							"    console.log(\"Bearer Token armazenado:\", jsonData.access_token);\r",
							"} else {\r",
							"    console.log(\"Token não encontrado na resposta.\");\r",
							"}\r",
							"\r",
							"// Verifica se a variável foi armazenada corretamente\r",
							"console.log(\"Token atual na variável:\", pm.environment.get(\"bearer_token\"));\r",
